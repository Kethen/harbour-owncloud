{
  "builder": "qmake",
  "framework": "ubuntu-sdk-20.04",
  "default": "clean build click-build",
  "kill": "harbour-owncloud",
  "dependencies_target": [
    "liblomiri-download-manager-client-dev",
    "libonline-accounts-qt-dev",
    "libgles-dev"
  ],
  "build_args": [
    "CONFIG+=nodaemon",
    "CONFIG+=nopermissionagent",
    "CONFIG+=noqmlextension",
    "CONFIG+=nosharing",
    "CONFIG+=quickcontrols",
    "CONFIG+=click"
  ],
  "make_jobs": 4,
  "qt_version": "5.12"
}
